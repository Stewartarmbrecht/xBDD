				<outline text="			};" />
				<outline text="		}" />
				<outline text="		public List<string> GetGeneratedReasons() {" />
				<outline text="			return new List<string>() {" />
				<outline text="				"Removing"," />
				<outline text="				"Untested"," />
				<outline text="				"Committed"," />
				<outline text="				"Ready"," />
				<outline text="				"Defining"," />
