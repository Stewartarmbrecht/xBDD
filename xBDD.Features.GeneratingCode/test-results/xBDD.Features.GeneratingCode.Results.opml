				<outline text=" File Path: ./MyGeneratedSample.Features/xBDDConfig.json" />
				<outline text="{" />
				<outline text="	"xBDD": {" />
				<outline text="		"TestRunName": "My Generated Sample - Features"," />
				<outline text="		"Browser": {" />
				<outline text="			"Watch": "false"" />
				<outline text="		}," />
				<outline text="		"HtmlReport": {" />
